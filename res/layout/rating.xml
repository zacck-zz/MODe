<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content">
	<TextView android:id="@+id/ratingText" android:layout_width="wrap_content" android:layout_height="wrap_content" />
	<TextView android:id="@+id/RsongDets" android:layout_width="wrap_content" android:layout_height="wrap_content" />
	<RatingBar android:id="@+id/ratingBar"
		android:isIndicator="false"
		android:stepSize="1.0"
		android:numStars="5"
		android:rating="0.0"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
	</RatingBar>

</LinearLayout>
